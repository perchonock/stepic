x = 0
y = 0
length = 5
direction = 1
num = 1
a = [[0]*length for i in range(length)]
#a[0][2] = 2
#for i in a:
  #  print(i)

while 0 <= y <= length-1:
        while 0 <= x + direction <= length-1:
            a[y][x] = num
            print(x)
            x = x + direction
            num +=1
        a[y][x] = num
        num +=1
        y = y + direction
        if y == 4:
            direction = -direction

for i in a:
    print(i)







